{
  "name": "starter-kit-core",
  "version": "1.0.0",
  "type": "module",
  "description": "Production-ready Eleventy starter for local service landing pages",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "eleventy --serve",
    "build": "eleventy",
    "generate": "node tools/generate-from-config.js",
    "generate:cli": "node tools/generate.js",
    "generate:offers": "node tools/generate-offers.mjs",
    "generate:demos": "node scripts/generate-demos.mjs",
